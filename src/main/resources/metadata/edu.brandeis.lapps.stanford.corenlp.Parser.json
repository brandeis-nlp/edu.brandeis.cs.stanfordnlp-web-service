{
  "$schema" : "http://vocab.lappsgrid.org/schema/service-schema-1.0.0.json",
  "name" : "edu.brandeis.lapps.stanford.corenlp.Parser",
  "version" : "2.2.0-SNAPSHOT",
  "description" : "This service is a wrapper around Stanford CoreNLP 3.3.1 providing a phrase structure parser service\nInternally it uses CoreNLP default \"tokenize\", \"ssplit\", \"parse\" annotators.",
  "vendor" : "http://www.cs.brandeis.edu/",
  "allow" : "http://vocab.lappsgrid.org/ns/allow#any",
  "license" : "http://vocab.lappsgrid.org/ns/license#apache-2.0",
  "requires" : {
    "encoding" : "UTF-8",
    "language" : [ "en" ],
    "format" : [ "http://vocab.lappsgrid.org/ns/media/text", "http://vocab.lappsgrid.org/ns/media/jsonld#lif" ]
  },
  "produces" : {
    "encoding" : "UTF-8",
    "language" : [ "en" ],
    "format" : [ "http://vocab.lappsgrid.org/ns/media/jsonld#lif" ],
    "annotations" : [ "http://vocab.lappsgrid.org/Constituent", "http://vocab.lappsgrid.org/Token", "http://vocab.lappsgrid.org/PhraseStructure" ]
  }
